  "main": "dist/utilities.ssr.js",
  "browser": "dist/utilities.esm.js",
  "module": "dist/utilities.esm.js",
  "unpkg": "dist/utilities.min.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup --config config/rollup.dev.js --watch",
    "build": "cross-env NODE_ENV=production rollup --config config/rollup.build.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config config/rollup.build.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config config/rollup.build.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config config/rollup.build.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-replace": "^2.2.1",
    "cross-env": "^6.0.3",
    "minimist": "^1.2.0",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3"
  },
  "engines": {
    "node": ">=8"
  }
}